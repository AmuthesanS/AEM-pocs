<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0" jcr:primaryType="cq:Page">
	<jcr:content
		cq:template="/apps/aempoc/templates/page-content"
		jcr:primaryType="cq:PageContent" jcr:title="File list"
		sling:resourceType="aempoc/components/structure/page">
		<logo jcr:primaryType="nt:unstructured"
			sling:resourceType="aempoc/components/structure/logo" />
		<title jcr:primaryType="nt:unstructured"
			sling:resourceType="aempoc/components/content/title" />
		<topnav jcr:primaryType="nt:unstructured"
			sling:resourceType="aempoc/components/structure/topnav" />
		<par jcr:primaryType="nt:unstructured"
			sling:resourceType="wcm/foundation/components/parsys">
			<text jcr:primaryType="nt:unstructured"
				sling:resourceType="aempoc/components/content/text"
				text="&lt;p>This is a file list component&lt;/p>" textIsRich="true" />
			<filelist jcr:primaryType="nt:unstructured"
				sling:resourceType="aempoc/components/content/filelist"
				rootPath="/content/geometrixx-media/en"
				tags="[geometrixx-media:entertainment/movies]" />
		</par>
	</jcr:content>
</jcr:root>
